<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title data-ng-bind="$state.current.data.pageTitle + ' - Optional text'">AqueDoc</title>
    <link rel="shortcut icon" type="image/x-icon" href="~/Content/favicon.ico" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <base href="@Url.Action("Index","Home")">
    @*<link href="~/Scripts/angular-filemanager/src/css/main.css" rel="stylesheet">*@
    <link href="~/Scripts/angular-filemanager/src/css/animations.css" rel="stylesheet">
    <link href="~/Scripts/angular-filemanager/src/css/dialogs.css" rel="stylesheet">
    
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/sbadmin")
    <link href="http://cdnjs.cloudflare.com/ajax/libs/summernote/0.8.1/summernote.css" rel="stylesheet" />
    <script src="http://cdnjs.cloudflare.com/ajax/libs/summernote/0.8.1/summernote.js"></script>
    @Scripts.Render("~/bundles/angular")
@*    @Scripts.Render("~/bundles/users")*@
@*    @Scripts.Render("~/bundles/addScripts")*@


@*    *@
    @Scripts.Render("~/bundles/aqueDocApp")

    <script src="~/Scripts/angular-filemanager/bower_components/angular-translate/angular-translate.min.js"></script>
    <script src="~/Scripts/angular-filemanager/bower_components/ng-file-upload/ng-file-upload.min.js"></script>

    <script src="~/Scripts/angular-filemanager/src/js/app.js"></script>
    <script src="~/Scripts/angular-filemanager/src/js/directives/directives.js"></script>
    <script src="~/Scripts/angular-filemanager/src/js/filters/filters.js"></script>
    <script src="~/Scripts/angular-filemanager/src/js/providers/config.js"></script>
    <script src="~/Scripts/angular-filemanager/src/js/entities/chmod.js"></script>
    <script src="~/Scripts/angular-filemanager/src/js/entities/item.js"></script>
