    <style>
        #context-menu-view {
            display: block !important;
        }
        .thumbnail {
            border: none !important;
        }
        
        .thumbnail.selected {
            background: #9cabb7 !important;
            color: white !important;
        }

        .col-md-3 {
            width: 25% !important;
            float: left !important;
        }

        .col-md-9 {
            width: 75% !important;
            float:left !important;
        }

        .col-120 {
            width: 100px;
            max-height: 100px;
            float: left;
            margin-bottom: 9px;
            margin-right: 9px;
        }

        textarea.form-control {
            height: 350px;
        }

        /*li > a {
            color: #4a4848;
        }*/

        .col-120 > a {
            color: #4a4848;
        }
    </style>
    

    

</head>

<body>

    <div ng-app="aqueDocApp">
        @RenderBody()
    </div>

    <div>
        @RenderSection("scripts", required: false)
    </div>
</body>
</html>
